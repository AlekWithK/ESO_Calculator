<TabbedPanelHeader>
	background_color: [.1,1,.8,1] #Button color	

<TabbedPanel>
	do_default_tab: False
	tab_width: (self.width - 6) / 3
	tab_pos: 'top_mid'
	
	TabbedPanelItem: #Parse Normalizer
		text: "Parse Normalizer"
		font_name: 'Impact'
		font_size: 19
		GridLayout:
			cols: 1
			padding: [150, 30]
			spacing: [0, 15]
			canvas.before:
				Color:
					rgba:[.21,.21,.21,1]
				Rectangle:
					size: self.size
					pos: self.pos
					source: 'Resources/gradient.jpg'		
			TextInput:
				id: t_dps
				multiline: False
				write_tab: False
				background_color: [1, 1, 1, .15]
				foreground_color: [1, 1, 1, 1]
				disabled_foreground_color: [1, 1, 1, 1]
				hint_text_color: [1, 1, 1, 1]
				cursor_color: [.1,1,.8,.4]

				input_filter: 'int'		
				hint_text: "Total DPS"
				tooltip_text: "Your total DPS"
				halign: 'center'
				font_size: 20			

				size_hint_x: None
				size_hint_y: None
				width: 200
				height: 40
			TextInput:
				id: c_chance
				multiline: False
				write_tab: False
				background_color: [1, 1, 1, .15]
				foreground_color: [1, 1, 1, 1]
				disabled_foreground_color: [1, 1, 1, 1]
				hint_text_color: [1, 1, 1, 1]
				cursor_color: [.1,1,.8,.4]

				input_filter: 'float'				
				hint_text: "Crit Chance"
				halign: 'center'
				font_size: 20

				size_hint_x: None
				size_hint_y: None
				width: 200
				height: 40
			TextInput:
				id: t_hits
				multiline: False
				write_tab: False
				background_color: [1, 1, 1, .15]
				foreground_color: [1, 1, 1, 1]
				disabled_foreground_color: [1, 1, 1, 1]
				hint_text_color: [1, 1, 1, 1]
				cursor_color: [.1,1,.8,.4]

				input_filter: 'int'		
				hint_text: "Total Hits"
				halign: 'center'
				font_size: 20			

				size_hint_x: None
				size_hint_y: None
				width: 200
				height: 40
			TextInput:
				id: t_c_hits
				multiline: False
				write_tab: False
				background_color: [1, 1, 1, .15]
				foreground_color: [1, 1, 1, 1]
				disabled_foreground_color: [1, 1, 1, 1]
				hint_text_color: [1, 1, 1, 1]
				cursor_color: [.1,1,.8,.4]

				input_filter: 'int'				
				hint_text: "Total Critical Hits"
				halign: 'center'
				font_size: 20

				size_hint_x: None
				size_hint_y: None
				width: 200
				height: 40
			TextInput:
				id: t_non_hits
				multiline: False
				write_tab: False
				background_color: [1, 1, 1, .15]
				foreground_color: [1, 1, 1, 1]
				disabled_foreground_color: [1, 1, 1, 1]
				hint_text_color: [1, 1, 1, 1]
				cursor_color: [.1,1,.8,.4]

				input_filter: 'int'		
				hint_text: "Total Non-Hits"
				halign: 'center'
				font_size: 20			

				size_hint_x: None
				size_hint_y: None
				width: 200
				height: 40
			TextInput:
				id: c_damage
				multiline: False
				write_tab: False
				background_color: [1, 1, 1, .15]
				foreground_color: [1, 1, 1, 1]
				disabled_foreground_color: [1, 1, 1, 1]
				hint_text_color: [1, 1, 1, 1]
				cursor_color: [.1,1,.8,.4]
				
				input_filter: 'float'				
				hint_text: "Critical Damage"
				halign: 'center'
				font_size: 20

				size_hint_x: None
				size_hint_y: None
				width: 200
				height: 40

			Label:
				canvas.before:
					Color:
						rgba:[.1,1,.8,.10]
					Rectangle:
						size: self.size
						pos: self.pos
					Line:
						width: 5
						rectangle: self.x, self.y, self.width, self.height

				id: result_lbl
				size_hint_y: None
				height: 150
				text: ""
				font_size: 24

			Button:
				padding: [20, 20]
				text: "Calculate"
				font_name: "Impact"
				font_size: 26
				background_color: [.1,1,.8,1]

				size_hint_x: None
				size_hint_y: None
				width: 200
				height: 50

				on_release: root.normalizer_pressed()

